			<column name="site_abb" header="SITE_ABB" type="STRING" />
			<column name="site_qw_id" header="SITE_QW_ID" type="STRING" />
			<column name="site_flow_id" header="SITE_FLOW_ID" type="STRING" />
			<column name="constit" header="CONSTIT" type="STRING" />
			<column name="date" header="DATE" type="DATE" />
			<column name="wy" header="WY" type="NUMERIC" />
			<column name="concentration" header="CONCENTRATION" type="NUMERIC" />
			<column name="remark" header="REMARK" type="STRING" />
		</loadData>
		<rollback>delete from discqw</rollback>
	</changeSet>
	<changeSet author="jiwalker" id="loadMflow" runOnChange="true">
		<delete tableName="mflow" />
