<?xml version="1.0" encoding="UTF-8"?> 
<databaseChangeLog 
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.2.xsd"
    logicalFilePath="root/changeLog.xml"> 

    <preConditions>
        <dbms type="postgresql" />
        <runningAs username="postgres" />
    </preConditions>
    <changeSet author="jiwalker" id="loadAflow" runOnChange="true">
		<delete tableName="aflow" />
		<loadData
			encoding="UTF-8"
			tableName="aflow"
			file="${nar.data.location}/aflow.csv"
		>
			<column name="rownum" header="" type="skip" />
			<column name="site_abb" header="SITE_ABB" type="STRING" />
			<column name="site_qw_id" header="SITE_QW_ID" type="STRING" />
			<column name="site_flow_id" header="SITE_FLOW_ID" type="STRING" />
			<column name="wy" header="WY" type="NUMERIC" />
			<column name="flow" header="FLOW" type="NUMERIC" />
		</loadData>
		<rollback>delete from aflow</rollback>
	</changeSet>
	<changeSet author="jiwalker" id="loadAloads" runOnChange="true">
		<delete tableName="aloads" />
		<loadData
			encoding="UTF-8"
