			<column name="constit" header="CONSTIT" type="STRING" />
			<column name="wy" header="WY" type="NUMERIC" />
			<column name="month" header="MONTH" type="NUMERIC" />
			<column name="modtype" header="MODTYPE" type="STRING" />
			<column name="tons" header="TONS" type="NUMERIC" />
			<column name="tons_l95" header="TONS_L95" type="NUMERIC" />
			<column name="tons_u95" header="TONS_U95" type="NUMERIC" />
		</loadData>
		<rollback>delete from mloads</rollback>
	</changeSet>
    
</databaseChangeLog>